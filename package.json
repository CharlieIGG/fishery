{
  "name": "fishery",
  "version": "0.0.2",
  "description": "A library for setting up JavaScript objects as test data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "testinit": "jest --init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thoughtbot/fishery.git"
  },
  "author": "Stephen Hanson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thoughtbot/fishery/issues"
  },
  "homepage": "https://github.com/thoughtbot/fishery#readme",
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "prettier": {
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "semi": true,
    "overrides": [
      {
        "files": "**/package.json",
        "options": {
          "parser": "json-stringify"
        }
      }
    ]
  }
}
